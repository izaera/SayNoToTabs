{
	"manifest_version": 2,
	"name": "SayNoToTabs",
	"short_name": "Max Tabs",
	"version": "1.0",
	"description": "Don't allow more than one tab per window.",
	"icons": {
		"48": "icon.svg",
		"96": "icon.svg"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"*://*/*",
		"tabs"
	],
	"applications": {
		"gecko": {
			"id": "SayNoToTabs@izaera.github.com"
		}
	}
}
